                <kanban create="false">
                    <field name="name"/>
                    <field name="company_id"/>
                    <field name="product_uom_qty"/>
<!--                    <field name="al_count"/>-->
<!--                    <field name="production_state"/>-->
<!--                    <field name="shipping_state"/>-->
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_global_click">
